CREATE TABLE sql_room (
  room_id TEXT PRIMARY KEY NOT NULL,
  room TEXT NOT NULL
);

getAllRooms:
SELECT * FROM sql_room;

getRoom:
SELECT * FROM sql_room
WHERE room_id = ?;

saveRoom:
INSERT OR REPLACE INTO sql_room
VALUES (?, ?);

deleteRoom:
DELETE FROM sql_room
WHERE room_id = ?;

deleteAllRooms:
DELETE FROM sql_room;
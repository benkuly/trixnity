CREATE TABLE sql_media_cache_mapping (
  cache_uri TEXT PRIMARY KEY NOT NULL,
  mxc_uri TEXT,
  size INTEGER,
  content_type TEXT
);

getUploadMedia:
SELECT * FROM sql_media_cache_mapping
WHERE cache_uri = ?;

saveUploadMedia:
INSERT OR REPLACE INTO sql_media_cache_mapping
VALUES (?, ?, ?, ?);

deleteUploadMedia:
DELETE FROM sql_media_cache_mapping
WHERE cache_uri = ?;

deleteAllUploadMedia:
DELETE FROM sql_media_cache_mapping;
